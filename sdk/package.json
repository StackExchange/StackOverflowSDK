{
  "name": "so-teams-sdk",
  "version": "1.0.1",
  "description": "SDK for Stack Overflow Teams API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "generateSDK": "openapi-generator-cli generate -i ./src/mock/swagger.json -g typescript -o ./src/generated",
    "build": "pnpm generateSDK && pnpm compile",
    "compile": "tsc",
    "clean": "rm -rf dist/",
    "prepublishOnly": "pnpm clean && pnpm build",
    "test": "npx tsx ./test.ts"
  },
  "keywords": [
    "stackoverflow",
    "teams",
    "api",
    "sdk"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "dotenv": "^17.2.1",
    "es6-promise": "^4.2.4",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.21.0",
    "@types/node": "^24.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}